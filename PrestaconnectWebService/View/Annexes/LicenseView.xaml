<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="PrestaconnectWebService.View.Annexes.LicenseView"
	x:Name="Window"
	Title="Gestion de la licence"
	Width="480" Height="156"
	WindowStyle="ThreeDBorderWindow" 
    Icon="/PrestaconnectWebService;component/Resources/prestaconnect.ico" 
    ResizeMode="NoResize" 
    WindowStartupLocation="CenterScreen"
	ShowInTaskbar="True"
    Background="{DynamicResource ResourceKey=WindowBackgroundBrush}" >
    <Grid>
        <Image Source="/PrestaconnectWebService;component/Resources/prestaconnect.ico" Margin="12,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Width="64" />
        <TextBlock Name="TextBlockLicense" Margin="86,0,12,0" TextAlignment="Justify" Height="Auto" VerticalAlignment="Center" TextWrapping="Wrap" FontSize="14"/>
    </Grid>
</Window>